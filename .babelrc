{
    "presets": [
        ["@babel/preset-env",
        {
            "targets":{
                "ie": "11"
            },
            "useBuiltIns": "entry",
            "corejs": {
                "version": 3,
                "proposals": true
            }
        }],
        "@babel/preset-react"
    ],
    "plugins": ["@babel/plugin-proposal-class-properties", "@babel/plugin-proposal-object-rest-spread"]
}