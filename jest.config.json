{
  "roots": ["<rootDir>/src"],
  "setupFilesAfterEnv": ["<rootDir>/setupEnzyme.js"],
  "moduleFileExtensions": ["js", "jsx", "json", "node"],
  "moduleNameMapper": {
    "^.+\\.(css|less|scss)$": "babel-jest",
    "^.+\\.(jpg|png|gif|svg)$": "identity-obj-proxy"
  },
  "snapshotSerializers": ["enzyme-to-json/serializer"]
}
